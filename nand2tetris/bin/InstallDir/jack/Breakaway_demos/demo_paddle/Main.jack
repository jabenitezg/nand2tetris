// Demo - moving Paddle

/**
 * The Main class initializes a new Paddle Demo game and starts it.
 */
class Main {

    /** Initializes a new game and starts it. */    
    function void main() {
        var PaddleDemo game;

        let game = PaddleDemo.new();
        do game.run();
        do game.dispose();

        return;
    }
}
